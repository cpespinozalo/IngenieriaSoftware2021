	<queryString>
		<![CDATA[SELECT ADO.CADOPCION,
       (SELECT NOMBRE FROM TITEM WHERE CODIGO=MAS.TIPOMASCOTA AND CCATALOGO=5007) TIPO,
       UPPER(MAS.NOMBRE) NOMBRE,
       MAS.EDAD || ' ' || DECODE(MAS.UNIDADEDAD,'A','AÑOS','M','MESES','') EDAD,
       (SELECT NOMBRE FROM TITEM WHERE CODIGO=MAS.GENERO AND CCATALOGO=5008)  GENERO,
       TO_CHAR(MAS.FPUBLICACION,'DD-MM-YYYY') FPUBLICACION,
       UPPER(USU.NOMBRES || ' ' || USU.APELLIDOS) USUARIO,
       TO_CHAR(ADO.FADOPCION,'DD-MM-YYYY') FADOPCION,
       UPPER(USD.NOMBRES || ' ' || USD.APELLIDOS) USUARIOADOPCION,
       USU.DIRECCION,
       (SELECT NOMBRE FROM TITEM WHERE CODIGO=USU.PROVINCIA AND CCATALOGO=5002)  PROVINCIA,
       (SELECT NOMBRE FROM TITEM WHERE CODIGO=USU.CIUDAD AND CODIGOPADRE=USU.PROVINCIA AND CCATALOGO=5003)  CIUDAD,
       (SELECT NOMBRE FROM TITEM WHERE CODIGO=ADO.ESTADOADOPCION AND CCATALOGO=5009) ESTADOADOPCION
FROM TMASCOTA MAS, TUSUARIO USU, TADOPCION ADO, TUSUARIO USD
WHERE MAS.CUSUARIOPUBLICACION=USU.CUSUARIO
AND ADO.CMASCOTA=MAS.CMASCOTA
AND ADO.CUSUARIOADOPCION=USD.CUSUARIO
AND UPPER(USD.NOMBRES || ' ' || USD.APELLIDOS) = $P{USUARIOMASCOTA}
ORDER BY ADO.FADOPCION]]>
	</queryString>
